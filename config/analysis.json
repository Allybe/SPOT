[
    {
        "type": "tmp",
        "name": "cycle",
        "outputPath": "cycle",
        "options": {
            "pickups": ["groundPickup","terminalPickup"],
            "scores": ["upperHub","lowerHub"],
            "misses": ["miss"]
        }
    },
    {
        "type": "team",
        "name": "aggregateArray",
        "outputPath": "cycle.all",
        "options": {
            "path": "cycle.all"
        }
    },
    {
        "type": "team",
        "name": "average",
        "outputPath": "cycle.averageTime",
        "options": {
            "path": "cycle.averageTime"
        }
    },
    {
        "type": "team",
        "name": "average",
        "outputPath": "cycle.averageTimeComplete",
        "options": {
            "path": "cycle.averageTimeComplete"
        }
    },
    {
        "type": "team",
        "name": "countActions",
        "outputPath": "counts",
        "options": {"all": true}
    },
    {
        "type": "team",
        "name": "ratio",
        "outputPath": "accuracy",
        "options": {
            "numerator": ["counts.lowerHub","counts.upperHub"],
            "denominator": ["counts.lowerHub","counts.upperHub","counts.miss"],
