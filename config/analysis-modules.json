[
    {
        "view": "team",
        "module": "HeatmapScatterPlot",
        "name": "Action Plot",
        "position": "main",
        "options": {
            "coordinatePath": "other.pos",
            "aggregatedActionsPath": "aggregatedActions",
            "actionGroups": [
                {
                    "name": "Shots",
                    "actions": ["upperHub", "lowerHub", "miss"]
                },
                {
                    "name": "Pickups",
                    "actions": ["groundPickup", "terminalPickup"]
                }
            ],
            "actionLabels": {
                "upperHub": "UpperH",
                "lowerHub": "LowerH",
                "miss": "Miss",
                "groundPickup": "GroundP",
                "terminalPickup": "TerminalP"
            },
            "imgPath": "img/field.svg"
        }
    },
    {
        "view": "team",
        "module": "Stats",
        "position": "side",
        "name": "Team Stats",
        "options": {
            "list": [
                {
                    "name": "Average Cycle Time",
                    "path": "cycle.averageTime",
                    "multiplier": 0.001,
                    "decimals": 2,
                    "unit": "s"
                },
                {
                    "name": "Average Completed Cycle Time",
                    "path": "cycle.averageTimeComplete",
                    "multiplier": 0.001,
                    "decimals": 2,
                    "unit": "s"
                },
                {
                    "name": "Average Accuracy",
                    "path": "accuracy",
                    "multiplier": 100,
                    "decimals": 1,
                    "unit": "%"
                },
                {
                    "name": "Average Scores",
                    "path": "scoreCount"
                },
                {
                    "name": "Average Misses",
                    "path": "counts.miss"
                }
            ]
        }
    },
    {
        "view": "team",
        "module": "Bar",
        "position": "main",
        "name": "Shot Distribution",
        "options": {
            "bars": [
                {
                    "name": "Upper Hub",
                    "path": "counts.upperHub"
                },
                {
                    "name": "Lower Hub",
                    "path": "counts.lowerHub"
                },
                {
                    "name": "Miss",
                    "path": "counts.miss"
                }
            ]
        }
    },
    {
        "view": "team",
        "module": "Pie",
        "position": "main",
        "name": "Pickup Distribution",
        "options": {
            "slices": [
                {
                    "name": "Ground",
                    "path": "counts.groundPickup"
                },
                {
                    "name": "Terminal",
                    "path": "counts.terminalPickup"
                }
            ]
        }
    },
