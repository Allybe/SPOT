[
    {
        "view": "team",
        "module": "HeatmapScatterPlots",
        "name": "Action Plot",
        "position": "main",
        "options": {
            "coordinatePath": "other.pos",
            "aggregatedActionsPath": "aggregatedActions",
            "actionGroups": [
                {
                    "label": "Shots",
                    "actions": ["upperHub", "lowerHub", "miss"]
                },
                {
                    "label": "Pickups",
                    "actions": ["groundPickup", "terminalPickup"]
                }
            ],
            "actionLabels": {
                "upperHub": "UpperH",
                "lowerHub": "LowerH",
                "miss": "Miss",
                "groundPickup": "GroundP",
                "terminalPickup": "TerminalP"
            }
        }
    },
    {
        "view": "team",
        "module": "Stats",
        "position": "side",
        "name": "Team Stats",
        "options": {
            "list": {
                "Average Cycle Time": {
                    "path": "cycle.averageTime",
                    "multiplier": 0.001,
                    "decimals": 2,
                    "unit": "s"
                },
                "Average Completed Cycle Time": {
                    "path": "cycle.averageTimeComplete",
                    "multiplier": 0.001,
                    "decimals": 2,
                    "unit": "s"
                },
                "Average Accuracy": {
                    "path": "accuracy",
                    "multiplier": 100,
                    "decimals": 1,
                    "unit": "%"
                },
                "Average Scores": {
                    "path": "scoreCount"
                },
                "Average Misses": {
                    "path": "counts.miss"
                }
            }
        }
    },
    {
        "view": "team",
        "module": "Bar",
        "position": "main",
        "name": "Shot Distribution",
        "options": {
            "bars": {
                "Upper Hub": {
                    "path": "counts.upperHub"
                },
                "Lower Hub": {
                    "path": "counts.lowerHub"
                },
                "Miss": {
                    "path": "counts.miss"
                }
            }
        }
    },
    {
        "view": "team",
        "module": "Pie",
        "position": "main",
        "name": "Pickup Distribution",
        "options": {
            "slices": {
                "Ground": {
                    "path": "counts.groundPickup"
                },
                "Terminal": {
                    "path": "counts.terminalPickup"
                }
            }
        }
    },
