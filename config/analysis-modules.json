[
    {
        "view": "team",
        "module": "HeatmapScatterPlot",
        "name": "Action Plot",
        "position": "main",
        "options": {
            "coordinatePath": "other.pos",
            "aggregatedActionsPath": "aggregatedActions",
            "actionGroups": [
                {
                    "name": "Shots",
                    "actions": ["upperHub", "lowerHub", "miss"]
                },
                {
                    "name": "Pickups",
                    "actions": ["groundPickup", "terminalPickup"]
                }
            ],
            "actionLabels": {
                "upperHub": "Upper",
                "lowerHub": "Lower",
                "miss": "Miss",
                "groundPickup": "Ground",
                "terminalPickup": "Terminal"
            },
            "imgPath": "img/field.svg"
        }
    },
    {
        "view": "team",
        "module": "Stats",
        "position": "side",
        "name": "Team Stats",
        "options": {
            "list": [
                {
                    "name": "Initiation Cross",
                    "path": "averages.tarmacCrossTime",
                    "multiplier": -0.001,
                    "addend": 150,
                    "decimals": 2,
                    "unit": "s in"
                },
                {
                    "name": "Climb Start Time",
                    "path": "averages.startClimbTime",
                    "multiplier": 0.001,
                    "decimals": 2,
                    "unit": "s to end"
                },
                {
                    "name": "Climb End Time",
                    "path": "averages.endClimbTime",
                    "multiplier": 0.001,
                    "decimals": 2,
                    "unit": "s to end"
                },
                {
                    "name": "Time Per Ball",
                    "path": "timePerBall",
                    "multiplier": 0.001,
                    "decimals": 2,
                    "unit": "s"
                },
                {
                    "name": "Accuracy",
                    "path": "accuracy",
                    "multiplier": 100,
                    "decimals": 1,
                    "unit": "%"
                },
                {
                    "name": "Average Auto Points",
                    "path": "averageScores.auto"
                },
                {
                    "name": "Average Climb Points",
                    "path": "averageScores.endgame"
                },
                {
                    "name": "Average Points",
                    "path": "averageScores.all"
                },
                {
                    "name": "Average Upper Hub Shots",
                    "path": "averages.upperHub"
                },
                {
                    "name": "Average Lower Hub Shots",
                    "path": "averages.lowerHub"
                },
                {
                    "name": "Average Misses",
                    "path": "averages.miss"
                }
            ]
        }
    },
    {
        "view": "team",
        "module": "Pie",
        "position": "main",
        "name": "Climb Distribution",
        "options": {
            "slices": [
                {
                    "name": "No Climb",
                    "path": "climbLevels.noClimb"
                },
                {
                    "name": "Fall",
                    "path": "climbLevels.climbF"
                },
                {
                    "name": "Low Rung",
                    "path": "climbLevels.climb1"
                },
                {
                    "name": "Mid Rung",
                    "path": "counts.climb2"
                },
                {
                    "name": "High Rung",
                    "path": "counts.climb3"
                },
                {
                    "name": "Traversal Rung",
                    "path": "counts.climb4"
                }
            ]
        }
    },
    {
        "view": "team",
        "module": "Pie",
        "position": "main",
        "name": "Shot Distribution",
        "options": {
            "slices": [
                {
                    "name": "Upper Hub",
                    "path": "counts.upperHub"
                },
                {
                    "name": "Lower Hub",
                    "path": "counts.lowerHub"
                },
                {
                    "name": "Miss",
                    "path": "counts.miss"
                }
            ]
        }
    },
    {
        "view": "team",
        "module": "Pie",
        "position": "main",
        "name": "Pickup Distribution",
        "options": {
            "slices": [
                {
                    "name": "Ground",
                    "path": "counts.groundPickup"
                },
                {
                    "name": "Terminal",
                    "path": "counts.terminalPickup"
                }
            ]
        }
    },


