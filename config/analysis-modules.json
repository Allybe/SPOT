[
    {
        "view": "team",
        "module": "HeatmapScatterPlot",
        "name": "Action Plot",
        "position": "main",
        "options": {
            "coordinatePath": "other.pos",
            "aggregatedActionsPath": "aggregatedActions",
            "actionGroups": [
                {
                    "name": "Shots",
                    "actions": ["upperHub", "lowerHub", "miss"]
                },
                {
                    "name": "Pickups",
                    "actions": ["groundPickup", "terminalPickup"]
                }
            ],
            "actionLabels": {
                "upperHub": "UpperH",
                "lowerHub": "LowerH",
                "miss": "Miss",
                "groundPickup": "GroundP",
                "terminalPickup": "TerminalP"
            },
            "imgPath": "img/field.svg"
        }
    },
    {
        "view": "team",
        "module": "Stats",
        "position": "side",
        "name": "Team Stats",
        "options": {
            "list": [
                {
                    "name": "Initiation Cross",
                    "path": "averages.tarmacCrossTime",
                    "multiplier": -0.001,
                    "addend": 150,
                    "decimals": 2,
                    "unit": "s in"
                },
                {
                    "name": "Climb Start",
                    "path": "averages.startClimbTime",
                    "multiplier": 0.001,
                    "decimals": 2,
                    "unit": "s to end"
                },
                {
                    "name": "All Cycle Time",
                    "path": "cycle.averageTime",
                    "multiplier": 0.001,
                    "decimals": 2,
                    "unit": "s"
                },
                {
                    "name": "Scored Cycle Time",
                    "path": "cycle.averageTimeComplete",
                    "multiplier": 0.001,
                    "decimals": 2,
                    "unit": "s"
                },
                {
                    "name": "Accuracy",
                    "path": "accuracy",
                    "multiplier": 100,
                    "decimals": 1,
                    "unit": "%"
                },
                {
                    "name": "Shot Score in Auto",
                    "path": "averageScores.auto"
                },
                {
                    "name": "Shot Score",
                    "path": "averageScores.all"
                },
                {
                    "name": "Upper Hub Shots",
                    "path": "averages.upperHub"
                },
                {
                    "name": "Lower Hub Shots",
                    "path": "averages.lowerHub"
                },
                {
                    "name": "Misses",
                    "path": "averages.miss"
                }
            ]
        }
    },
    {
        "view": "team",
        "module": "Pie",
        "position": "main",
        "name": "Climb Distribution",
        "options": {
            "slices": [
                {
                    "name": "Climb 1",
                    "path": "counts.climb1"
                },
                {
                    "name": "Climb 2",
                    "path": "counts.climb2"
                },
                {
                    "name": "Climb 3",
                    "path": "counts.climb3"
                },
                {
                    "name": "Climb 4",
                    "path": "counts.climb4"
                }
            ]
        }
    },
    {
        "view": "team",
        "module": "Bar",
        "position": "main",
        "name": "Shot Distribution",
        "options": {
            "bars": [
                {
                    "name": "Upper Hub",
                    "path": "counts.upperHub"
                },
                {
                    "name": "Lower Hub",
